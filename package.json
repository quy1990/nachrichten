{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "profiler-server": "node node_modules/laravel-profiler-client/server/server.js http=8099 ws=1901",
        "profiler-client": "http-server node_modules/laravel-profiler-client/dist/ -o -s",
        "ps": "npm run profiler-server",
        "pc": "npm run profiler-client",
        "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
    },
    "devDependencies": {
        "axios": "^0.23.0",
        "bootstrap": "^5.1.3",
        "jquery": "^3.6",
        "laravel-mix": "^6.0.6",
        "laravel-profiler-client": "^1.3.0",
        "lodash": "^4.17.19",
        "popper.js": "^1.16.1",
        "postcss": "^8.1.14",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.32.11",
        "sass-loader": "^12.2.0",
        "vue": "^2.6.14",
        "vue-loader": "^15.9.7",
        "vue-router": "^3.5.2",
        "vue-template-compiler": "^2.6.14"
    },
    "dependencies": {
        "browser-sync": "^2.27.5",
        "browser-sync-webpack-plugin": "^2.3.0",
        "vue-axios": "^3.3.7"
    }
}
